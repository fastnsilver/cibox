{
  "builders": [
    {
      "type": "vmware-iso",
      "boot_command": [
        "<esc>",
        "<wait>linux ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ks.cfg<enter>"
      ],
      "headless" : false,
      "boot_wait": "12s",
      "disk_size": 10140,
      "guest_os_type": "linux",
      "http_directory": "./http",
      "iso_checksum": "08961a5cb32d2cdf72026bec43876b7f",
      "iso_checksum_type": "md5",
      "iso_url": "iso/rhel-server-7.0-x86_64-dvd.iso",
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "ssh_wait_timeout": "20m",
      "vmx_data": {
        "memsize": "4096",
        "numvcpus": "4",
        "cpuid.coresPerSocket": "1"
      }
    }
  ],
  "provisioners": [
  	{
  		"type": "file",
  		"source": "scripts/rhcp.properties",
  		"destination": "/home/vagrant/rhcp.properties"
	},
    {
      "execute_command": "echo 'vagrant'|{{.Vars}} sudo -E -S sh '{{.Path}}'",
      "type": "shell",
      "scripts": [
        "scripts/setup.sh",
        "scripts/vmware-guest-additions.sh",
        "scripts/vagrant.sh",
        "scripts/compact.sh"
      ] 
    }
  ],
  "post-processors": [
    {
     "type": "vagrant",
     "compression_level": 9,
     "output": "rhel-7.0-x64_vmware.box"
    }
  ]
}